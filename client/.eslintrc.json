{
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "ignorePatterns": [
      "**/*.spec.ts",
      "**/*.spec.tsx"
    ],

    "parser": "@typescript-eslint/parser",
    "rules": {
      "@typescript-eslint/no-unused-vars": ["error"],
      "@typescript-eslint/no-explicit-any": "warn",
      "react/prop-types": "off",
      "no-console": "warn"
    }
  }
  